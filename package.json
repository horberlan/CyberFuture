  "module": "dist/index.js",
  "keywords": [
    "React",
    "Cyber",
    "Futuristic",
    "Component",
    "Styled-system",
    "Cyberfuture",
    "CyberButton",
    "Card",
    "CardCarousel",
    "CyberInput",
    "CSS variables",
    "customization",
    "theme",
    "responsive"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/horberlan/CyberFuture"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
